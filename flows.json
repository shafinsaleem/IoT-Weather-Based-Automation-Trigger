    {
        "id": "7125a02ba92b521a",
        "type": "function",
        "z": "d5d5643f9101182e",
        "name": "function 4",
        "func": "// Extract the first object from the payload\nlet temperatureData = msg.payload[0];\n// Ensure the temperature value is available and is a number\nif (temperatureData && typeof temperatureData._value === 'number') {\n    let temperature = temperatureData._value;\n    // If the temperature is above 22Â°C, send \"ON\", else send \"OFF\"\n    msg.payload = (temperature > 22) ? \"ON\" : \"OFF\";\n} else {\n    msg.payload = \"Error: No valid temperature data found\"; // Handle missing or invalid temperature data\n}\n// Return the modified message\nreturn msg;",
        "outputs": 1,
        "timeout": 0,
        "noerr": 0,
